{
  "id": "about",
  "label": "About",
  "format": "custom",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "profile_image": {
        "type": "Image",
        "config": {
          "label": "Profile Image",
          "constraint": {
            "width": 800,
            "height": 800
          },
          "thumbnails": []
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "Über mich",
          "allowTargetBlank": false,
          "single": "heading1"
        }
      },
      "lead_text": {
        "type": "StructuredText",
        "config": {
          "label": "Lead Text",
          "placeholder": "Introductory text...",
          "allowTargetBlank": false,
          "single": "paragraph"
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "label": "Main Content",
          "placeholder": "Main content with headings and paragraphs...",
          "allowTargetBlank": true,
          "multi": "paragraph,heading2,heading3,strong,em,hyperlink,list-item,o-list-item"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "type": "Text",
        "config": {
          "label": "Meta Title",
          "placeholder": "Über mich - Kindertagespflege Hummelgarten"
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": "Lernen Sie mich kennen..."
        }
      }
    }
  }
}